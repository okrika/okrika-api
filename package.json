{
  "name": "okrika-api",
  "version": "0.0.1",
  "description": "API Provider for Okrika",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "fix": "yarn fix:format && yarn fix:lint",
    "fix:format": "prettier --write \"src/**/*.ts\"",
    "fix:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "9.2.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/throttler": "^6.2.1",
    "@supercharge/request-ip": "^1.2.0",
    "axios": "^1.7.7",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.4.0",
    "dotenv": "^16.4.5",
    "geoip-country": "^5.0.202409132342",
    "googleapis": "^144.0.0",
    "graphql": "^16.9.0",
    "graphql-type-object-id": "^1.0.0",
    "libphonenumber-js": "^1.11.8",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "6.8.1",
    "nanoid": "^3.1.22",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "platform": "^1.3.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/geoip-country": "^4.0.2",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^16.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/platform": "^1.3.6",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "28.0.3",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
